local ScreenGui = Instance.new("ScreenGui")
local Main = Instance.new("ScrollingFrame")
local glockanims = Instance.new("TextButton")
local shottyanims = Instance.new("TextButton")
local thestreetsscript = Instance.new("TextButton")
local TextLabel = Instance.new("TextLabel")
local close = Instance.new("TextButton")
local openmain = Instance.new("Frame")
local open = Instance.new("TextButton")

--Properties:

ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
ScreenGui.ResetOnSpawn = false

Main.Name = "Main"
Main.Parent = ScreenGui
Main.Active = true
Main.BackgroundColor3 = Color3.fromRGB(155, 15, 255)
Main.Position = UDim2.new(0.467990041, 0, 0.352640539, 0)
Main.Size = UDim2.new(0, 275, 0, 223)
Main.Visible = false
Main.Active = true
Main.Draggable = true

glockanims.Name = "glockanims"
glockanims.Parent = Main
glockanims.BackgroundColor3 = Color3.fromRGB(74, 74, 74)
glockanims.Position = UDim2.new(0.109090909, 0, 0.096678026, 0)
glockanims.Size = UDim2.new(0, 200, 0, 50)
glockanims.Font = Enum.Font.SourceSans
glockanims.Text = "glock anims -- made by lynxzzizle"
glockanims.TextColor3 = Color3.fromRGB(0, 0, 0)
glockanims.TextSize = 14.000
glockanims.MouseButton1Down:connect(function()
	game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(KeyPressed)
		if KeyPressed == "w" then
			local lplr = game:GetService("Players").LocalPlayer
			local Glock = lplr.Backpack:FindFirstChild("Glock")    
			local Animation = Instance.new("Animation")
			Animation.AnimationId = "rbxassetid://889968874"
			local plr = game:GetService('Players').LocalPlayer
			local HUM = plr.Character.Humanoid:LoadAnimation(Animation)        
			Glock.Equipped:connect(function()
				wait(.1)
				HUM:Play()
				HUM:AdjustSpeed(0.0)
			end)
			Glock.Unequipped:connect(function()
				HUM:Stop()
			end)
			local lplr = game:GetService("Players").LocalPlayer
			local Shotty = lplr.Backpack:FindFirstChild("Glock")    
			local Animation = Instance.new("Animation")
			Animation.AnimationId = "rbxassetid://889968874"
			local plr = game:GetService('Players').LocalPlayer
			local HUM = plr.Character.Humanoid:LoadAnimation(Animation)        
			Shotty.Equipped:connect(function()
				wait(.1)
				HUM:Play()
				HUM:AdjustSpeed(0.0)
			end)
			Shotty.Unequipped:connect(function()
				HUM:Stop()
			end)
		end
	end)

	game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(KeyPressed)
		if KeyPressed == "s" then
			local lplr = game:GetService("Players").LocalPlayer
			local Glock = lplr.Backpack:FindFirstChild("Glock")    
